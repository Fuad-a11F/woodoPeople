{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsc --project tsconfig.server.json && cross-env NODE_ENV=development EXTERNAL_SERVER_URL='http://localhost:3001' INTERNAL_SERVER_URL='http://localhost:3001' node server/index.js",
    "build": "tsc --project tsconfig.server.json && vite build --config vite.config.ts && vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build:ssr": "tsc && vite build --config ssr.config.ts",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "jest ./"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.1.0",
    "@mui/icons-material": "^6.1.4",
    "@mui/material": "^6.1.4",
    "@reduxjs/toolkit": "^2.3.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "@types/react-redux": "^7.1.34",
    "@types/redux": "^3.6.0",
    "dotenv": "^16.0.2",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.7.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.27.0",
    "redux": "^5.0.1",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@types/jest": "29.5.14",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "^8.23.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.0.1",
    "lefthook": "^1.3.9",
    "prettier": "^2.7.1",
    "sass": "^1.80.3",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2",
    "vite": "^3.0.7"
  },
  "license": "MIT"
}
